{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prefer-stateless-function": false,
    "react/prop-types": false
  },
  "env": {
    "es6": true,
    "node": true
  }
}